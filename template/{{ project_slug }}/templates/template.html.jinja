{% raw %}{% load i18n static sekizai_tags{% endraw %}{% if project_type == "django-cms" %}{% raw %} cms_tags{% endraw %}{% endif %}{% raw %}%}
<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7 ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8 ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <title>{% block title %}{% endblock %}</title>
  {% block head_meta %}
  <meta name="description" content="{% block meta-description %}{% endblock %}">
  <meta name="author" content="{% block meta-author %}{% endblock %}">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {% endblock %}

  {% block css %}
  {% addtoblock "css" %}
  <link rel="stylesheet" type="text/css" href="css/all.css" />
  {% endaddtoblock %}
  {% block extra-css %}
  {% endblock %}
  {% addtoblock "css" %}
  {% endaddtoblock %}
  {% endblock %}

  {% render_block "css" %}

  <script src="{% endraw %}{{ project_slug }}{% raw %}/libs/modernizr/modernizr-3.5.0.min.js"></script>
  {% block js %}{% endraw %}{% raw %}
  {% block jquery %}
  {% addtoblock "js" %}
  <script src="{% endraw %}{{ project_slug }}{% raw %}/libs/jquery/jquery-3.3.1.min.js"></script>
  {% endaddtoblock %}
  {% endblock %}
  {% block javascript %}
  {% addtoblock "js" %}
  <script src="{% endraw %}{{ project_slug }}{% raw %}/js/plugins.js" defer></script>
  <script src="{% endraw %}{{ project_slug }}{% raw %}/js/script.js" defer></script>
  {% endaddtoblock %}
  {% endblock %}
  {% block extra-js %}
  {% endblock %}
  {% endblock %}
</head>

<body>{% endraw %}{% if project_type == "django-cms" %}{% raw %}{% cms_toolbar %}{% endraw %}{% endif %}{% raw %}{% block ie7 %}
  <!--[if lt IE 8]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> to experience this site.</p><![endif]-->{% endblock %}
    <header>
      {% block header %}
      {% endblock %}
    </header>
    <section id="main" class="main" role="main">
      {% block main %}
      {% endblock %}
    </section>

    <footer>
      {% block footer %}
      {% endblock %}
    </footer>
  </div> <!--! end of #container -->

  {% render_block "js" %}
</body>
</html>{% endraw %}
